<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Online Banking</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>Registration</servlet-name>
    <servlet-class>com.Regis</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Registration</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Registration2</servlet-name>
    <servlet-class>com.Regis2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Registration2</servlet-name>
    <url-pattern>/registration2</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.LogOut</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Transfer</servlet-name>
    <servlet-class>com.Transfer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Transfer</servlet-name>
    <url-pattern>/tran</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Transaction</servlet-name>
    <servlet-class>com.Transaction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Transaction</servlet-name>
    <url-pattern>/trans</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ALogin</servlet-name>
    <servlet-class>com.AdminLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ALogin</servlet-name>
    <url-pattern>/alogin</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DAdmin</servlet-name>
    <servlet-class>com.Delete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DAdmin</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>
</web-app>